{
  "name": "ical.js",
  "version": "2.0.0",
  "author": "Philipp Kewisch",
  "contributors": [
    "Github Contributors (https://github.com/kewisch/ical.js/graphs/contributors)"
  ],
  "description": "Javascript parser for ics (rfc5545) and vcard (rfc6350) data",
  "main": "dist/ical.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kewisch/ical.js.git"
  },
  "keywords": [
    "calendar",
    "iCalendar",
    "jCal",
    "vCard",
    "jCard",
    "parser"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@octokit/core": "^4.1.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/benchmark": "^2.1.2",
    "@types/chai": "^4.3.3",
    "@types/mocha": "github:alex-kinokon/mocha-types",
    "@types/node": "^18.11.7",
    "benchmark": "^2.1.4",
    "c8": "^7.11.0",
    "chai": "^4.2.0",
    "eslint": "^8.9.0",
    "jsdoc": "^3.6.10",
    "minami": "^1.2.3",
    "mocha": "^10.0.0",
    "node-fetch": "^3.2.0",
    "prettier": "^2.7.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "yauzl-promise": "^2.1.3"
  },
  "license": "MPL-2.0",
  "engine": {
    "node": ">=10"
  },
  "scripts": {
    "test": "npm run test-unit && npm run test-acceptance",
    "test-unit": "c8 mocha",
    "test-acceptance": "mocha test/acceptance/*_test.ts",
    "test-performance": "mocha --reporter test/support/perfReporter.ts test/performance/*_test.ts",
    "test-all": "npm run test-unit && npm run test-acceptance && npm run test-performance",
    "build": "rollup -c",
    "lint": "eslint lib/ical test tools/scriptutils.js",
    "jsdoc": "rm -rf docs/api && jsdoc -R README.md -d docs/api -t node_modules/minami lib/ical/*.js",
    "validator": "sed -e 's#unpkg.com/ical.js#unpkg.com/ical.js@'`grep '\"version\"' package.json | cut -d '\"' -f 4`'/dist/ical.js#' tools/validator.html > docs/validator.html && echo 'Validator written to docs/validator.html'",
    "ghpages": "npm run jsdoc && npm run validator"
  },
  "exports": {
    "import": "dist/ical.min.js",
    "require": "dist/ical.es5.min.cjs"
  },
  "files": [
    "dist/ical.js",
    "dist/ical.min.js",
    "dist/ical.es5.cjs",
    "dist/ical.es5.min.cjs",
    "lib/ical/*.js"
  ],
  "c8": {
    "include": "lib/ical",
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "saucelabs": {
    "SL_Chrome": {
      "base": "SauceLabs",
      "browserName": "chrome"
    },
    "SL_Firefox": {
      "base": "SauceLabs",
      "browserName": "firefox"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "upstream": {
    "repository": "https://github.com/kewisch/ical.js.git",
    "branch": "main",
    "commit": "3f0838cc27a7e82d9e61f548dc1cc91d7cf2d546"
  }
}
