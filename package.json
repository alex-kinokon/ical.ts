{
  "name": "ical.js",
  "version": "2.0.0",
  "author": "Philipp Kewisch",
  "contributors": [
    "Github Contributors (https://github.com/kewisch/ical.js/graphs/contributors)"
  ],
  "description": "Javascript parser for ics (rfc5545) and vcard (rfc6350) data",
  "main": "dist/ical.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kewisch/ical.js.git"
  },
  "keywords": [
    "calendar",
    "iCalendar",
    "jCal",
    "vCard",
    "jCard",
    "parser"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@octokit/core": "^4.2.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/benchmark": "^2.1.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "github:alex-kinokon/mocha-types",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.54.0",
    "benchmark": "^2.1.4",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "esbuild": "^0.17.16",
    "esbuild-register": "^3.4.2",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "jsdoc": "^4.0.2",
    "minami": "^1.2.3",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-ts": "^3.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yauzl-promise": "^2.1.3"
  },
  "license": "MPL-2.0",
  "engine": {
    "node": ">=10"
  },
  "scripts": {
    "test": "npm run test-unit && npm run test-acceptance",
    "test-unit": "c8 mocha",
    "test-acceptance": "mocha test/acceptance/*_test.ts",
    "test-performance": "mocha --reporter test/support/perfReporter.ts test/performance/*_test.ts",
    "test-all": "npm run test-unit && npm run test-acceptance && npm run test-performance",
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint lib/ical test tools/scriptutils.js",
    "jsdoc": "rm -rf docs/api && jsdoc -R README.md -d docs/api -t node_modules/minami lib/ical/*.js",
    "validator": "sed -e 's#unpkg.com/ical.js#unpkg.com/ical.js@'`grep '\"version\"' package.json | cut -d '\"' -f 4`'/dist/ical.js#' tools/validator.html > docs/validator.html && echo 'Validator written to docs/validator.html'",
    "ghpages": "npm run jsdoc && npm run validator"
  },
  "exports": {
    "import": "dist/ical.min.js",
    "require": "dist/ical.es5.min.cjs"
  },
  "files": [
    "dist/ical.js",
    "dist/ical.min.js",
    "dist/ical.es5.cjs",
    "dist/ical.es5.min.cjs",
    "lib/ical/*.js"
  ],
  "c8": {
    "include": "lib/ical",
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "saucelabs": {
    "SL_Chrome": {
      "base": "SauceLabs",
      "browserName": "chrome"
    },
    "SL_Firefox": {
      "base": "SauceLabs",
      "browserName": "firefox"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "upstream": {
    "version": 1,
    "sources": {
      "main": {
        "repository": "https://github.com/kewisch/ical.js.git",
        "branch": "main",
        "commit": "3f0838cc27a7e82d9e61f548dc1cc91d7cf2d546"
      }
    }
  },
  "eslintignore": [
    "dist"
  ]
}
