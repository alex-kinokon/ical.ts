{
  "name": "ical.js",
  "version": "0.1.0",
  "author": "alex-kinokon",
  "contributors": [
    "Philipp Kewisch",
    "Github Contributors (https://github.com/kewisch/ical.js/graphs/contributors)"
  ],
  "description": "Javascript parser for ics (rfc5545) and vcard (rfc6350) data",
  "main": "dist/ical.cjs",
  "module": "dist/ical.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kewisch/ical.js.git"
  },
  "exports": {
    ".": {
      "default": "./dist/ical.cjs",
      "import": "./dist/ical.js"
    },
    "./timezones": {
      "default": "./dist/ical.timezones.js"
    }
  },
  "keywords": [
    "calendar",
    "iCalendar",
    "jCal",
    "vCard",
    "jCard",
    "parser"
  ],
  "devDependencies": {
    "@octokit/core": "^4.2.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/benchmark": "^2.1.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "github:alex-kinokon/mocha-types",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.1",
    "benchmark": "^2.1.4",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "esbuild": "^0.17.16",
    "esbuild-register": "^3.4.2",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-ts": "^3.2.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4",
    "yauzl-promise": "^2.1.3"
  },
  "license": "MPL-2.0",
  "engine": {
    "node": ">=10"
  },
  "scripts": {
    "test": "npm run test-unit && npm run test-acceptance",
    "test-unit": "c8 mocha",
    "test-acceptance": "mocha test/acceptance/*_test.ts",
    "test-performance": "mocha --reporter test/support/perfReporter.ts test/performance/*_test.ts",
    "test-all": "npm run test-unit && npm run test-acceptance && npm run test-performance",
    "build": "rollup -c --bundleConfigAsCjs",
    "lint": "eslint lib/ical test tools/scriptutils.js",
    "jsdoc": "rm -rf docs; typedoc --includeVersion --skipErrorChecking --out docs lib/ical/index.ts",
    "ghpages": "npm run jsdoc"
  },
  "files": [
    "dist"
  ],
  "c8": {
    "include": "lib/ical",
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  },
  "saucelabs": {
    "SL_Chrome": {
      "base": "SauceLabs",
      "browserName": "chrome"
    },
    "SL_Firefox": {
      "base": "SauceLabs",
      "browserName": "firefox"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "upstream": {
    "version": 1,
    "sources": {
      "main": {
        "repository": "https://github.com/kewisch/ical.js.git",
        "branch": "main",
        "commit": "ac6e5124884b6aa2eacb1f87a9d85d51425b8fc3"
      }
    }
  },
  "eslintignore": [
    "dist"
  ]
}
